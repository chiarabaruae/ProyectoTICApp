<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.miapp.ActivityList">

    <RelativeLayout
        android:id="@+id/SeccListadoObjetivos"
        android:layout_width="420dp"
        android:layout_height="60dp"
        android:background="#9DA993">

        <TextView

            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="15dp"
            android:text="Listado de Objetivos"
            android:textSize="20dp"
            android:textStyle="bold"></TextView>

    </RelativeLayout>
    <RelativeLayout
        android:layout_width="420dp"
        android:layout_height="550dp"
        android:layout_below="@+id/SeccListadoObjetivos">

        <ListView
            android:id="@+id/listadoObjetivos"
            android:layout_width="404dp"
            android:layout_height="550dp"
            android:layout_below="@id/SeccListadoObjetivos"
            tools:ignore="MissingConstraints,NotSibling"
            tools:layout_editor_absoluteX="4dp"
            tools:layout_editor_absoluteY="5dp" />

    </RelativeLayout>


</RelativeLayout>